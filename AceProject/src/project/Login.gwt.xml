<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='aceproject'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <!-- BELOW ONLY ALLOWS CERTAIN BROWSERS, SPEEDS UP COMPILATION -->
  <!-- DELETE LINE WHEN DONE TESTING -->
  <set-property name="user.agent" value="gecko1_8"/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- inherits name='com.google.gwt.user.theme.clean.Clean'-->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <!-- Below is for AceEditor -->
	<inherits name='edu.ycp.cs.dh.acegwt.AceGWT'/>
	<!-- Below is for server side loggin -->
	<inherits name="com.google.gwt.logging.Logging"/>
	<!-- Below is for sliders -->
	<inherits name='com.google.gwt.gen2.Gen2'/>
  <!-- Specify the app entry point class.                         -->
  <entry-point class='project.client.AceProject'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

  
    <!--  Disables annoying logger popup -->
  	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED"/>
  	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
  	<set-property name="gwt.logging.consoleHandler" value="DISABLED" />
  	 
  	 
  	
</module>
